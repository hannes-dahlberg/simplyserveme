{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [
                255
            ]
        },
        "no-shadowed-variable": false,
        "no-console": false,
        "variable-name": {
            "options": "allow-leading-underscore"
        },
        "object-literal-sort-keys": false,
        "unified-signatures": false
    },
    "rulesDirectory": [],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.spec.ts"
    ]
}